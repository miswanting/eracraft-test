{
  "name": "eracraft",
  "version": "0.1.14-beta.5",
  "private": true,
  "packageManager": "pnpm@10.6.5",
  "description": "",
  "author": "",
  "license": "ISC",
  "keywords": [],
  "main": "index.js",
  "scripts": {
    "icon": "cd packages/backend && pnpm icon",
    "dev": "cd packages/backend && pnpm dev",
    "build": "cd packages/backend && pnpm build",
    "docs:dev": "cd packages/frontend && pnpm docs:dev",
    "docs:build": "cd packages/frontend && pnpm docs:build",
    "clean": "cd packages/frontend && pnpm clean && cd ../backend && pnpm clean && cd ../.. && rimraf -g ./**/icons ./**/gen ./**/node_modules",
    "commit": "cz",
    "bump": "bumpp -r --no-commit --no-tag --no-push",
    "changelog": "pnpm conventional-changelog -p angular",
    "bump:commit": "echo PLS Choose `as-is` && bumpp -a --no-push",
    "push:tags": "git push --tags",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "bumpp": "^10.2.3",
    "commitizen": "^4.3.1",
    "conventional-changelog": "^7.1.1",
    "conventional-changelog-angular": "^8.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.34.0",
    "esno": "^4.8.0",
    "rimraf": "^6.0.1",
    "taze": "^19.4.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
